{"Comment": "Rules for the regular expression crossword at http://boingboing.net/2013/02/11/regular-expressions-crossword.html",
 "Left to right": [
   ".*H.*H.*",
   "(DI|NS|TH|OM)*",
   "F.*[AO].*[AO].*",
   "(O|RHH|MM)*",
   ".*",
   "C*MC(CCC|MM)*",
   "[^C]*[^R]*III.*",
   "(...?)\\1*",
   "([^X]|XCC)*",
   "(RR|HHH)*.?",
   "N.*X.X.X.*E",
   "R*D*M*",
   ".(C|HH)*"],
 "Bottom right to top left": [
   ".*G.*V.*H.*",
   "[CR]*",
   ".*XEXM*",
   ".*DD.*CCM.*",
   ".*XHCR.*X.*",
   ".*(.)(.)(.)(.)\\4\\3\\2\\1.*",
   ".*(IN|SE|HI)",
   "[^C]*MMM[^C]*",
   ".*(.)C\\1X\\1.*",
   "[CEIMU]*OH[AEMOR]*",
   "(RX|[^R])*",
   "[^M]*M[^M]*",
   "(S|MM|HHH)*"],
 "Top right to bottom left": [
   ".*SE.*UE.*",
   ".*LR.*RL.*",
   ".*OXR.*",
   "([^EMC]|EM)*",
   "(HHX|[^HX])*",
   ".*PRR.*DDC.*",
   ".*",
   "[AM]*CM(RC)*R?",
   "([^MC]|MM|CC)*",
   "(E|CR|MN)*",
   "P+(..)\\1.*",
   "[CHMNOR]*I[CHMNOR]*",
   "(ND|ET|IN)[^X]*"]}
